<% if(errors.length > 0) { %>
<div class="div_error">
    <p class="error">
        <% errors.forEach(erro => { %>
            <%= erro %> <br>
        <% }); %>
    </p>
</div>
<% } %>

<% if(success.length > 0) { %>
<div class="div_success">
    <p class="success">
        <% success.forEach(success => { %>
            <%= success %> <br>
        <% }); %>
    </p>
</div>
<% } %>